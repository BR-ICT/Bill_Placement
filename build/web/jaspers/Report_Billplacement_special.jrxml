	<parameter name="startdate" class="java.lang.String"/>
	<parameter name="enddate" class="java.lang.String"/>
	<queryString>
		<![CDATA[SELECT * FROM (
--BILL
SELECT CUSTOMER.OKCUNO,BPM_RINV,master.BPM_TYPE,BPM_REMARK,customer.OKCUNM,TRIM(customer.oksmcd) AS oksmcd ,customer.okpycd,customer.okcfc3
,'BILL' AS DATEDETAIL
,SUBSTRING(bill.BPH_BPDT,7,2)||'/' ||SUBSTRING(bill.BPH_BPDT,5,2)|| '/' ||SUBSTRING(bill.BPH_BPDT,1,4) AS Dateloc
, BPH_BPDT||'A' AS sortingorder,BPH_BPDT AS rawdate,
 LEFT(XMLSERIALIZE(XMLAGG(XMLTEXT(SUBSTRING(bill.BPH_STDT,7,2)||'/' ||SUBSTRING(bill.BPH_STDT,5,2) 
|| '-' || SUBSTRING(bill.BPH_FNDT,7,2) ||'/' ||SUBSTRING(bill.BPH_FNDT,5,2) ||',')) AS VARCHAR(50)), LENGTH(XMLSERIALIZE(XMLAGG(XMLTEXT(SUBSTRING(bill.BPH_STDT,7,2)||'/' ||SUBSTRING(bill.BPH_STDT,5,2) 
|| '-' || SUBSTRING(bill.BPH_FNDT,7,2) ||'/' ||SUBSTRING(bill.BPH_FNDT,5,2) ||',')) AS VARCHAR(50))) - 1 ) AS INVOICE
,BPM_BILDESC, BPM_COLDESC, BPM_COLBY
FROM BRLDTA0100.BP_HBILL bill
LEFT JOIN M3FDBPRD.OCUSMA customer ON  CUSTOMER.OKCONO = BILL.BPH_CONO  AND CUSTOMER.OKCUNO = BILL.BPH_CUNO 
JOIN BRLDTA0100.BP_MASTER master ON master.BPM_CONO  = bill.BPH_CONO  AND master.BPM_CUNO = bill.BPH_CUNO
WHERE (BPH_BPDT BETWEEN $P{startdate} AND $P{enddate}) 
and (master.BPM_RINV <> 7 AND master.BPM_RINV <> 15 AND master.BPM_RINV <> 30)
GROUP BY  OKCUNO,BPM_TYPE,OKCUNM,OKSMCD,OKPYCD,OKCFC3,BPH_BPDT,BPM_BILDESC, BPM_COLDESC, BPM_COLBY,BPM_RINV,BPM_REMARK
--PAY
UNION
SELECT CUSTOMER.OKCUNO,BPM_RINV,master.BPM_TYPE,BPM_REMARK,customer.OKCUNM,TRIM(customer.oksmcd) AS oksmcd,customer.okpycd,customer.okcfc3
,CASE WHEN BPH_CLDT BETWEEN $P{startdate} AND $P{enddate}
THEN 'PAY'
ELSE '' END AS Datedetail,
CASE WHEN BPH_CLDT BETWEEN $P{startdate} AND $P{enddate}
THEN SUBSTRING(bill.BPH_CLDT,7,2)||'/' ||SUBSTRING(bill.BPH_CLDT,5,2)|| '/' ||SUBSTRING(bill.BPH_CLDT,1,4)
ELSE '' END AS Dateloc
,CASE WHEN BPH_CLDT BETWEEN $P{startdate} AND $P{enddate}
THEN bill.BPH_CLDT||'B'
ELSE '' END AS sortingorder,
bill.BPH_CLDT AS rawdate,
 LEFT(XMLSERIALIZE(XMLAGG(XMLTEXT(SUBSTRING(bill.BPH_STDT,7,2)||'/' ||SUBSTRING(bill.BPH_STDT,5,2) 
|| '-' || SUBSTRING(bill.BPH_FNDT,7,2) ||'/' ||SUBSTRING(bill.BPH_FNDT,5,2) ||',')) AS VARCHAR(50)), LENGTH(XMLSERIALIZE(XMLAGG(XMLTEXT(SUBSTRING(bill.BPH_STDT,7,2)||'/' ||SUBSTRING(bill.BPH_STDT,5,2) 
|| '-' || SUBSTRING(bill.BPH_FNDT,7,2) ||'/' ||SUBSTRING(bill.BPH_FNDT,5,2) ||',')) AS VARCHAR(50))) - 1 ) AS INVOICE
,BPM_BILDESC, BPM_COLDESC, BPM_COLBY
FROM BRLDTA0100.BP_HBILL bill
LEFT JOIN M3FDBPRD.OCUSMA customer ON  CUSTOMER.OKCONO = BILL.BPH_CONO  AND CUSTOMER.OKCUNO = BILL.BPH_CUNO 
JOIN BRLDTA0100.BP_MASTER master ON master.BPM_CONO  = bill.BPH_CONO AND master.BPM_CUNO = bill.BPH_CUNO
LEFT JOIN BRLDTA0100.H_BILLNOTE billnote ON billnote.HB_CUNO = bill.BPH_CUNO 
AND bill.BPH_CONO = billnote.HB_CONO AND billnote.HB_TRDT = bill.BPH_BPDT
WHERE (BPH_CLDT BETWEEN $P{startdate} AND $P{enddate}) and (master.BPM_RINV <> 7 AND master.BPM_RINV <> 15 AND master.BPM_RINV <> 30)
GROUP BY OKCUNO,BPM_TYPE,OKCUNM,OKSMCD,OKPYCD,OKCFC3,BPH_CLDT ,BPM_BILDESC, BPM_COLDESC, BPM_COLBY,BPM_RINV,BPM_REMARK
UNION
SELECT CUSTOMER.OKCUNO,BPM_RINV,master.BPM_TYPE,BPM_REMARK,customer.OKCUNM,TRIM(customer.oksmcd) AS oksmcd,customer.okpycd,customer.okcfc3
,CASE WHEN BPH_CLDT BETWEEN $P{startdate} AND $P{enddate}
THEN 'Amount'
ELSE '' END AS Datedetail,
CASE WHEN BPH_CLDT BETWEEN $P{startdate} AND $P{enddate}
THEN SUBSTRING(bill.BPH_CLDT,7,2)||'/' ||SUBSTRING(bill.BPH_CLDT,5,2)|| '/' ||SUBSTRING(bill.BPH_CLDT,1,4)
ELSE '' END AS Dateloc
,CASE WHEN BPH_CLDT BETWEEN $P{startdate} AND $P{enddate}
THEN bill.BPH_CLDT||'C'
ELSE '' END AS sortingorder,bill.BPH_CLDT AS rawdate,
LEFT(XMLSERIALIZE(XMLAGG(XMLTEXT(HB_BNNO||','))AS VARCHAR(50)), LENGTH(XMLSERIALIZE(XMLAGG(XMLTEXT(HB_BNNO||','))AS VARCHAR(50))) - 1)
 AS INVOICE
,BPM_BILDESC, BPM_COLDESC, BPM_COLBY
FROM BRLDTA0100.BP_HBILL bill
LEFT JOIN M3FDBPRD.OCUSMA customer ON  CUSTOMER.OKCONO = BILL.BPH_CONO  AND CUSTOMER.OKCUNO = BILL.BPH_CUNO 
JOIN BRLDTA0100.BP_MASTER master ON master.BPM_CONO  = bill.BPH_CONO AND master.BPM_CUNO = bill.BPH_CUNO
 JOIN BRLDTA0100.H_BILLNOTE billnote ON billnote.HB_CUNO = bill.BPH_CUNO 
AND bill.BPH_CONO = billnote.HB_CONO AND billnote.HB_TRDT = bill.BPH_BPDT
WHERE (BPH_CLDT BETWEEN $P{startdate} AND $P{enddate}) and (master.BPM_RINV <> 7 AND master.BPM_RINV <> 15 AND master.BPM_RINV <> 30)
GROUP BY OKCUNO,BPM_TYPE,OKCUNM,OKSMCD,OKPYCD,OKCFC3,BPH_CLDT ,BPM_BILDESC, BPM_COLDESC, BPM_COLBY,BPM_RINV,BPM_REMARK
UNION 
SELECT BPM_CUNO AS OKCUNO,BPM_RINV,master.BPM_TYPE,BPM_REMARK,customer.OKCUNM, trim(OKSMCD) AS OKSMCD, OKPYCD, OKCFC3,'NODETAIL',  '0', 'A',0 AS RAWDATE, 'X',BPM_BILDESC, BPM_COLDESC, BPM_COLBY FROM BRLDTA0100.BP_MASTER master
LEFT JOIN M3FDBPRD.OCUSMA customer ON  CUSTOMER.OKCONO = MASTER.BPM_CONO  AND CUSTOMER.OKCUNO = master.BPM_CUNO 
WHERE (master.BPM_RINV <> 7 AND master.BPM_RINV <> 15 AND master.BPM_RINV <> 30)
AND BPM_CUNO NOT IN 
(SELECT BPH_CUNO FROM BRLDTA0100.BP_HBILL)
ORDER BY OKCUNO
) AS A
ORDER BY SORTINGORDER]]>
	</queryString>
